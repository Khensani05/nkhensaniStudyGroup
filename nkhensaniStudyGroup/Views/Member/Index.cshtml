@model IEnumerable<nkhensaniStudyGroup.Models.Member>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>STUDY GROUP MEMBERS</h1>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StdNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StdName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StdSurname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StdEmail)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StdNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StdName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StdSurname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StdEmail)
            </td>
        </tr>
        
    }
</table>
